    "react": "^15.4",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.4",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "recursive-readdir-sync": "^1.0.6",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "2.5.3",
    "sass-loader": "^4.0.0",
    "simple-assign": "^0.1.0",
    "sinon": "^1.17.3",
    "slick-carousel": "^1.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-combine-loaders": "^2.0.0",
    "webpack-dev-server": "^1.15.0"
  }
}
