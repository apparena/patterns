{
  "name": "apparena-patterns",
  "version": "2.4.2",
  "license": "MIT",
  "description": "Corporate identity portal and React pattern library of App-Arena.com",
  "keywords": [
    "apparena",
    "styleguide",
    "patterns",
    "components",
    "reactjs",
    "react"
  ],
  "engines": {
    "node": ">=7.0"
  },
  "scripts": {
    "build": "babel-node build/generator/index.js && webpack --progress --color --display-error-details --config build/webpack/webpack.config.dev.js && babel ./source/patterns --extensions \".tsx,.jsx,.js\" --ignore *.md,*.txt,*.mustache,*.spec.js,*.json --out-dir ./dist/apparena-patterns-react/patterns --copy-files && babel-node ./build/generator/copy-files.js",
    "build:prod": "babel-node build/generator/index.js && webpack --progress --color --display-error-details --config build/webpack/webpack.config.prod.js && babel ./source/patterns --extensions \".tsx,.jsx,.js\" --ignore *.md,*.txt,*.mustache,*.spec.js,*.json --out-dir ./dist/apparena-patterns-react/patterns --copy-files && babel-node ./build/generator/copy-files.js",
    "build:stats": "webpack --profile --progress --json > stats.json --config build/webpack/webpack.config.prod.js",
    "publish:generator:apparena-pattern": "npm publish source/generator-apparena-pattern",
    "start": "webpack-dev-server --progress --color --display-error-details --config build/webpack/webpack.config.dev.js --open",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc",
    "build:patterns": "babel ./source/patterns --extensions \".tsx,.jsx,.js\" --ignore *.md,*.txt,*.mustache,*.spec.js,*.json --out-dir ./dist/apparena-patterns-react/patterns --copy-files",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apparena/patterns.git"
  },
