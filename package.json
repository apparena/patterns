{
  "name": "apparena-patterns",
  "version": "1.0.38",
  "license": "MIT",
  "description": "Styleguide and patterns of App-Arena.com",
  "keywords": [
    "apparena",
    "styleguide",
    "patterns",
    "components",
    "reactjs",
    "react"
  ],
  "engines": {
    "node": ">=7.0"
  },
  "scripts": {
    "build": "npm run build:babel && npm run build:copy-files && npm run build:umd",
    "build:babel": "babel ./source/_patterns --ignore *.md,*.txt,*.mustache,*.spec.js,*.json  --out-dir ./build/apparena-patterns-react/_patterns --copy-files",
    "build:copy-files": "babel-node ./build/scripts/copy-files.js",
    "build:docs": "babel-node $NODE_DEBUG_OPTION build/generator/index.js",
    "build:docs:all": "npm run build:docs && webpack --progress --config build/generator/frontend/build/webpack/webpack.config.js",
    "build:light": "npm run clean:build:all && npm run build:babel && npm run build:copy-files",
    "build:umd": "webpack -p --config build/webpack/webpack.config.prod.js",
    "clean:build:all": "rimraf build/apparena-patterns-react",
    "prebuild": "npm run clean:build",
    "publish": "cd build/apparena-patterns-react && npm publish",
    "start": "node build/webpack/server.js",
    "test": "cross-env NODE_ENV=test nyc ava",
    "test:coverage": "nyc report --reporter=lcov --reporter=json --reporter=text-lcov --reporter=html",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apparena/patterns.git"
  },
  "peerDependencies": {
    "react": "^15.6",
    "react-dom": "^15.6"
  },
