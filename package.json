  "scripts": {
    "build": "babel-node build/generator/index.js && webpack --progress --color --display-error-details --config build/webpack/webpack.config.dev.js && babel ./source/patterns --ignore *.md,*.txt,*.mustache,*.spec.js,*.json  --out-dir ./dist/apparena-patterns-react/patterns --copy-files && babel-node ./build/generator/copy-files.js",
    "build:prod": "babel-node build/generator/index.js && webpack --progress --color --display-error-details --config build/webpack/webpack.config.prod.js && babel ./source/patterns --ignore *.md,*.txt,*.mustache,*.spec.js,*.json  --out-dir ./dist/apparena-patterns-react/patterns --copy-files && babel-node ./build/generator/copy-files.js",
    "publish:generator:apparena-pattern": "npm publish source/generator-apparena-pattern",
    "start": "webpack-dev-server --progress --color --display-error-details --config build/webpack/webpack.config.dev.js --open",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
