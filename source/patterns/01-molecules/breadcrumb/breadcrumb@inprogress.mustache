<ol class="breadcrumb">
    {{# breadcrumbs }}
        {{#isActive}}
            <li class="breadcrumb-item active">{{ text }}</li>
        {{/isActive}}
        {{^ isActive }}
            <li class="breadcrumb-item"><a href="{{ url }}">{{ text }}</a></li>
        {{/isActive}}
    {{/ breadcrumbs }}
</ol>