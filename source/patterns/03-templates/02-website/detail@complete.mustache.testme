<script type="application/javascript">
    var servicePackageSlider = document.getElementById('servicePackageSlider'),
            servicePackageValue  = document.getElementById('servicePackageValue'),
            licenseSlider        = document.getElementById('licenseSlider'),
            licenseValue         = document.getElementById('licenseValue');

    noUiSlider.create(licenseSlider, {
        start : 1,
        step  : 1,
        format: wNumb({
            decimals: 0
        }),
        range : {
            'min': 1,
            'max': 24
        },
        pips  : {
            mode   : 'steps',
            density: 24,
            format : wNumb({
                decimals: 0
            })
        }
    });

    // Note how these are 'string' values, not numbers.
    licenseSlider.noUiSlider.on('update', function (values, handle) {
        //licenseValue.innerHTML = values + ' Tage';
    });

    noUiSlider.create(servicePackageSlider, {
        start : 0,
        step  : 1,
        format: wNumb({
            decimals: 0
        }),
        range : {
            'min': 0,
            'max': 24
        },
        pips  : {
            mode   : 'steps',
            density: 24,
            format : wNumb({
                decimals: 0
            })
        }
    });

    // Note how these are 'string' values, not numbers.
    servicePackageSlider.noUiSlider.on('update', function (values, handle) {
        //servicePackageValue.innerHTML = values + ' Tage';
    });

</script>

